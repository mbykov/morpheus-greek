{
  "name": "morpheus-greek",
  "version": "0.3.4",
  "description": "Morpheus for ancient greek based on electron.js",
  "main": "main.js",
  "scripts": {
      "build": "webpack",
      "rebuild_": "electron-rebuild -f -w leveldown",
      "postinstall_": "electron-builder install-app-deps",
      "start": "electron .",
      "test": "echo \"Error: no test specified\"",
      "pack": "electron-builder --dir",
      "dist": "electron-builder",
      "release": "electron-builder"
  },
  "homepage": "http://diglossa.org",
  "repository": "https://github.com/mbykov/morpheus-greek",
  "keywords": [
    "chinese",
    "segmentation",
    "node.js",
    "electron.js"
  ],
  "author": "mbykov <m.bykov@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.5",
    "electron": "^1.7.5",
    "electron-builder": "^19.22.1",
    "electron-rebuild": "^1.6.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "antrax": "^0.1.78",
    "component-classes": "^1.2.6",
    "component-emitter": "^1.2.1",
    "component-events": "^1.0.10",
    "electron-is-dev": "^0.1.2",
    "electron-log": "^2.2.6",
    "electron-updater": "^2.4.1",
    "electron-window-state": "^4.1.1",
    "fs-jetpack": "^1.0.0",
    "screen": "^0.2.10",
    "lodash": "^4.17.4",
    "pouchdb": "^6.3.4",
    "underscore": "^1.8.3"
  },
  "build": {
    "appId": "com.github.mbykov.morpheus-greek",
      "files": [
      "build/",
      "assets/",
      "lib/",
      "main.js",
      "contents.js",
      "contents.css",
      "index.html",
      "package.json"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "mbykov",
        "repo": "morpheus-greek"
      }
    ],
    "mac": {
      "category": "Educational software",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "linux": {
      "category": "Educational software",
        "icon": "assets",
        "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
        "icon": "assets/icon.ico",
        "publisherName": "Michael Bykov"
    },
    "nsis": {
      "perMachine": false
    }
  }
}
