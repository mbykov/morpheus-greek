{
    "name": "electron",
    "version": "0.2.9",
    "description": "Morpheus for ancient greek based on electron.js",
    "main": "main.js",
    "scripts": {
        "postinstall": "electron-rebuild",
        "start": "electron .",
        "dev": "electron . --debug",
        "lint": "standard | snazzy"
    },
    "repository": "https://github.com/mbykov/morpheus-gr",
    "keywords": [
        "ancient",
        "greek",
        "morphology",
        "syntax",
        "node.js",
        "electron.js"
    ],
    "author": "m.bykov@gmail.com",
    "license": "GPL-3.0",
    "devDependencies": {
        "electron": "^1.6.6",
        "electron-builder": "^17.3.1",
        "electron-rebuild": "^1.5.11"
  },
    "dependencies": {
        "electron-updater": "^1.15.0",
        "electron-log": "^2.2.6",
        "component-classes": "^1.2.6",
        "component-emitter": "^1.2.1",
        "component-events": "^1.0.10",
        "electron-window-state": "^4.1.1",
        "orthos": "^0.2.1",
        "underscore": "^1.8.3"
    },
    "build": {
        "appId": "com.github.mbykov.electron",
        "mac": {
            "category": "Educational software",
            "target": [
                "zip",
                "dmg"
            ]
        },
        "nsis": {
            "perMachine": true,
            "installerIcon": "build/icon.ico"
        }
    }
}
